FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (15 0 0);

boundaryField
{
    inlet
    {
        type            freestreamVelocity;
        freestreamValue $internalField;
    }

    outlet
    {
        type            freestreamVelocity;
        freestreamValue $internalField;
    }

    frontAndBack
    {
        type            empty;
    }

    topAndBottom
    {
    	type			slip;
    }

    CAD
    {
    	type			noSlip;
	}
    AMI1
    {
        type            cyclicAMI;
        value           uniform (0 0 0);
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform (0 0 0);
    }
    // AMI
    // {
    //     type            cyclicAMI;
    //     value           uniform 0;
    // }
    // AMI_slave
    // {
    //     type            cyclicAMI;
    //     value           uniform 0;
    // }
}

